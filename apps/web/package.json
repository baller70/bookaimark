{
  "name": "@bookaimark/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "NODE_OPTIONS=\"--max-old-space-size=4096\" next dev --port 3000",
    "dev:prewarm": "concurrently \"npm run dev:fast\" \"node scripts/prewarm-pages.js\"",
    "dev:ultra": "NODE_OPTIONS='--max-old-space-size=16384' next dev --turbo",
    "dev:monitor": "concurrently \"npm run dev:fast\" \"sleep 30 && node scripts/performance-monitor.js\"",
    "prewarm": "node scripts/prewarm-pages.js",
    "monitor": "node scripts/performance-monitor.js",
    "perf": "npm run prewarm && npm run monitor",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "reset": "npm run clean && npm install",
    "setup": "node scripts/setup-dev-environment.js",
    "summary": "node scripts/optimization-summary.js",
    "dev:clean": "rm -rf .next && npm run dev:fast",
    "build:fast": "NODE_OPTIONS=\"--max-old-space-size=4096\" next build"
  },
  "dependencies": {
    "@bookaimark/ai-copilot": "workspace:*",
    "@bookaimark/analytics": "workspace:*",
    "@bookaimark/auth": "workspace:*",
    "@bookaimark/dashboard": "workspace:*",
    "@bookaimark/dna-profile": "workspace:*",
    "@bookaimark/marketplace": "workspace:*",
    "@bookaimark/oracle": "workspace:*",
    "@bookaimark/notifications": "workspace:*",
    "@bookaimark/pomodoro": "workspace:*",
    "@bookaimark/kanban": "workspace:*",
    "@bookaimark/simple-board": "workspace:*",
    "@bookaimark/media": "workspace:*",
    "@bookaimark/timeline": "workspace:*",
    "@bookaimark/playbooks": "workspace:*",
    "@bookaimark/settings": "workspace:*",
    "@bookaimark/shared": "workspace:*",
    "@bookaimark/ui": "workspace:*",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.0.18",
    "@sentry/nextjs": "^9.33.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.47.2",
    "next": "15.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.10.0",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "concurrently": "^8.2.2",
    "typescript": "5.3.2"
  }
}